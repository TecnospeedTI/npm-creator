{
  "name": "npm-creator",
  "version": "0.2.8",
  "description": "Gerador de projetos para o NPM",
  "bin": "./bin/npm-creator",
  "scripts": {
    "lint": "standard --fix",
    "test": "npm run lint && NODE_ENV=test jest --coverage",
    "test-windows": "npm run lint && set NODE_ENV=test && jest --coverage"
  },
  "standard": {
    "globals": [
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "describe",
      "it",
      "test",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TecnospeedTI/npm-creator.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TecnospeedTI/npm-creator/issues"
  },
  "homepage": "https://github.com/TecnospeedTI/npm-creator#readme",
  "dependencies": {
    "async": "^2.5.0",
    "chalk": "^2.0.1",
    "cli-spinner": "^0.2.6",
    "minimist": "^1.2.0",
    "mv": "^2.1.1",
    "os-locale": "^2.0.0",
    "request": "^2.81.0",
    "rmdir": "^1.2.0",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "express": "^4.15.3",
    "jest": "^20.0.4",
    "standard": "^10.0.2"
  }
}
